{
  "executionRoleArn": "ecs-task-execution-role-dol-ui-claimant-intake-server-ENVIRONMENT_PLACEHOLDER",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ENVIRONMENT_PLACEHOLDER/dol-ui-claimant-intake-APP_PLACEHOLDER",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "dol-ui-claimant-intake-APP_PLACEHOLDER-PR_PLACEHOLDER"
        }
      },
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "aws,aws-ENVIRONMENT_PLACEHOLDER"
        },
        {
          "name": "JAVA_TOOL_OPTIONS",
          "value": "-javaagent:/app/aws-opentelemetry-agent.jar"
        },
        {
          "name": "OTEL_RESOURCE_ATTRIBUTES",
          "value": "service.name=dol-ui-claimant-intake-server-ENVIRONMENT_PLACEHOLDER"
        }
      ],
      "mountPoints": [
        {
          "containerPath": "/tmp/app",
          "sourceVolume": "app_tmp"
        }
      ],
      "secrets": [
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/database-host",
          "name": "DB_HOST"
        },
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/database-port",
          "name": "DB_PORT"
        },
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/database-name",
          "name": "DB_NAME"
        },
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/loops-host",
          "name": "WGPM_HOST"
        },
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/loops-path",
          "name": "WGPM_PATH"
        },
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/s3-claims-bucket",
          "name": "S3_CLAIMS_BUCKET"
        },
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/s3-claims-bucket-kms-key",
          "name": "S3_CLAIMS_BUCKET_KMS_KEY"
        },
        {
          "valueFrom": "/dol-ui-claimant-intake-ENVIRONMENT_PLACEHOLDER/cognito-issuer",
          "name": "COGNITO_ISSUER"
        }
      ],
      "volumesFrom": [],
      "image": null,
      "dependsOn": [
        {
          "containerName": "aws-otel-collector",
          "condition": "START"
        }
      ],
      "essential": true,
      "readonlyRootFilesystem": true,
      "name": "dol-ui-claimant-intake-server-ENVIRONMENT_PLACEHOLDER-PR_PLACEHOLDER"
    },
    {
      "name": "aws-otel-collector",
      "image": null,
      "command": ["--config=/etc/ecs/ecs-default-config.yaml"],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ENVIRONMENT_PLACEHOLDER/aws-otel-collector",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [ "/healthcheck" ],
        "interval": 5,
        "timeout": 6,
        "retries": 5,
        "startPeriod": 1
      },
      "readonlyRootFilesystem": true
    }
  ],
  "placementConstraints": [],
  "memory": "2048",
  "taskRoleArn": "ecs-task-role-dol-ui-claimant-intake-server-ENVIRONMENT_PLACEHOLDER",
  "family": "dol-ui-claimant-intake-server-ENVIRONMENT_PLACEHOLDER-PR_PLACEHOLDER",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "volumes": [
    {
      "name": "app_tmp"
    }
  ]
}
